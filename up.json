{
  "name": "collected-source",
  "profile": "default",
  "regions": [
    "us-east-2"
  ],
  "hooks": {
    "build": "make"
  },
  "proxy": {
    "command": "./node-v8.9.4-linux-x64/bin/node src/server.js"
  },
  "stages": {
    "staging": {
      "domain": "staging.1.source.collected.design"
    },
    "production": {
      "domain": "1.source.collected.design"
    }
  },
  "error_pages": {
    "disable": true
  }
}